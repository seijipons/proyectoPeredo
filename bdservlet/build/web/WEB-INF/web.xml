<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>bd.Login</servlet-class>
        <init-param>
            <param-name>Controlador</param-name>
            <param-value>com.mysql.jdbc.Driver</param-value>
        </init-param>
        <init-param>
            <param-name>Bd</param-name>
            <param-value>jdbc:mysql://localhost/prueba</param-value>
        </init-param>
        <init-param>
            <param-name>Usuario</param-name>
            <param-value>root</param-value>
        </init-param>
        <init-param>
            <param-name>Password</param-name>
            <param-value/>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Alta</servlet-name>
        <servlet-class>bd.Alta</servlet-class>
        <init-param>
            <param-name>Controlador</param-name>
            <param-value>com.mysql.jdbc.Driver</param-value>
        </init-param>
        <init-param>
            <param-name>Bd</param-name>
            <param-value>jdbc:mysql://localhost:3306/prueba</param-value>
        </init-param>
        <init-param>
            <param-name>Usuario</param-name>
            <param-value>root</param-value>
        </init-param>
        <init-param>
            <param-name>Password</param-name>
            <param-value></param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Alta</servlet-name>
        <url-pattern>/Alta</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
